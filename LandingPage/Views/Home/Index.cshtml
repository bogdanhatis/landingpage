        </div>
    </div>
</section>
<!-- end team -->

<section id="video" class="section-spacing bg-img">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div id="video-carousel" class="carousel slide" data-ride="carousel" data-pause="false">
                <div class="carousel-inner">
                    @foreach (var video in listVideos.Where(x => x.Name == "Video"))
                    {
                        var _OrderIndex = video.OrderIndex;
                        var activeVideo = listVideos.Where(x => x.Name == "Video").Min(x => x.OrderIndex) == video.OrderIndex ? "active" : "";
                        <div class="item text-center @activeVideo">
                            <div class="col-xs-6">
                                <div class="play-button text-center">
                                    <a href="#" data-toggle="modal" data-target="#video-modal-@_OrderIndex"><i class="icon-video"></i></a>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="download-info fadeInRight">
                                    <h3>@HtmlHelperForView.ImageViewHelper(listVideos, 1, _OrderIndex, "VideoTitle")</h3>
                                    <p>@HtmlHelperForView.ImageViewHelper(listVideos, 2, _OrderIndex, "VideoDescription")</p>
                                </div>
                            </div>
                        </div>
                    }
                </div>
                <ol class="video-carousel-indicators">
                    @{
                        var countVideos = listVideos.Where(x => x.Name == "Video").Count();
                        for (int i = 0; i < countVideos; i++)
                        {
                            var active = i == 0 ? "active" : "";
                            <li data-target="#video-carousel" data-slide-to="@i" class="@active"></li>
                        }
                    }
                </ol>
            </div>
        </div>
    </div>
</section>


<!-- end fun facts -->

<section class="section-spacing" id="pricing">
    <div class="container">
        <div class="row">
            <div class="section-title text-center">
                <h2>@HtmlHelperForView.ViewHelper(Model, 43, "HeaderDescription")</h2>
                <p>@HtmlHelperForView.ViewHelper(Model, 43, "HeaderSubtitle")</p>
            </div>
        </div>
        <div class="row">
            @foreach (var price in listPrices.Where(x => x.Name == "Prices"))
            {
                var _OrderIndex = price.OrderIndex;
            <div class="col-xs-12 col-sm-4">
                
                    
                <div class="price-table text-center">
                    
                        
                        <div class="pricing-header">
                            <h3>@HtmlHelperForView.ImageViewHelper(listPrices, 1, _OrderIndex, "Prices")</h3>
                            <h4>@HtmlHelperForView.ImageViewHelper(listPrices, 1, _OrderIndex, "Money")</h4>
                        </div>
                        <ul class="pricing-list">
                            @foreach(var item in HtmlHelperForView.ImageViewHelper(listPrices, 1, _OrderIndex, "Features").Split(';'))
                            {
                                <li>@item</li>
                            }
                            </ul>
                    <div class="offer-action">
                        <a href="#" class="btn btn-primary">
                            <span>@HtmlHelperForView.ViewHelper(Model, 43, "Button")</span>
                        </a>
                    </div>
                    
                </div>
            </div>
            }
        </div>
    </div>
</section>
<!-- end pricing -->

