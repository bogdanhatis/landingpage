<section class="section-spacing" id="team">
    <div class="container">
        <div class="row">
            <div class="section-title text-center">
                <h2>@HtmlHelperForView.ViewHelper(Model, 39, "HeaderDescription")</h2>
                <p>@HtmlHelperForView.ViewHelper(Model, 39, "HeaderSubtitle")</p>
            </div>
        </div>
        @{
            var memberName = HtmlHelperForView.ImageViewHelper(Model, 1, 39, "MemberName");
            var memberImage = HtmlHelperForView.ImageViewHelper(Model, 4, 39, "MemberImage");
            var memberSocialMediaFacebook = HtmlHelperForView.ImageViewHelper(Model, 1, 39, "MemberSocialMediaFacebook");

        }



        <div class="row">

            <div class="col-xs-12">
                <div class="owl-carousel" id="member-carousel">
                    @foreach (var item in listCmsDetails.Where(x => x.Name == "MemberName"))
                    {
                        var _OrderIndex = item.OrderIndex;
                        <div class="team-member">
                            <div class="team-member wow fadeIn">
                                <div class="team-thumb">

                                    <img src="@HtmlHelperForView.ImageViewHelper(listCmsDetails, 1, _OrderIndex, "MemberImage")" alt="MemberImage">

                                    <div class="contact-bio">
                                        <ul>

                                            <li><a href="@HtmlHelperForView.ImageViewHelper(listCmsDetails, 1, _OrderIndex, "MemberSocialMediaFacebook")"><i class="fa fa-facebook"></i></a></li>

                                            <li><a href="@HtmlHelperForView.ImageViewHelper(listCmsDetails, 1, _OrderIndex, "MemberSocialMediaTwitter")"><i class="fa fa-twitter"></i></a></li>

                                            <li><a href="@HtmlHelperForView.ImageViewHelper(listCmsDetails, 1, _OrderIndex, "MemberSocialMediaLinkedin")"><i class="fa fa-linkedin"></i></a></li>

                                            <li><a href="@HtmlHelperForView.ImageViewHelper(listCmsDetails, 1, _OrderIndex, "MemberSocialMediaInstagram")"><i class="fa fa-instagram"></i></a></li>

