            styles: [{ "featureType": "administrative", "elementType": "labels.text.fill", "stylers": [{ "color": "#444444" }] }, { "featureType": "administrative.locality", "elementType": "labels.text.stroke", "stylers": [{ "visibility": "on" }] }, { "featureType": "administrative.locality", "elementType": "labels.icon", "stylers": [{ "visibility": "on" }, { "color": "#f1c40f" }] }, { "featureType": "landscape", "elementType": "all", "stylers": [{ "color": "#f2f2f2" }] }, { "featureType": "poi", "elementType": "all", "stylers": [{ "visibility": "off" }] }, { "featureType": "road", "elementType": "all", "stylers": [{ "saturation": -100 }, { "lightness": 45 }] }, { "featureType": "road.highway", "elementType": "all", "stylers": [{ "visibility": "simplified" }] }, { "featureType": "road.arterial", "elementType": "labels.icon", "stylers": [{ "visibility": "off" }] }, { "featureType": "transit", "elementType": "all", "stylers": [{ "visibility": "off" }] }, { "featureType": "water", "elementType": "all", "stylers": [{ "color": "#9e9e9e" }, { "visibility": "on" }] }]
        };

        var map = new google.maps.Map(document.getElementById('map'),
            mapOptions);
        var marker = new google.maps.Marker({
            position: map.getCenter(),
            animation: google.maps.Animation.BOUNCE,
            icon: '/img/map-marker.png',
            map: map
        });
    }
    google.maps.event.addDomListener(window, 'load', initialize);
    </script>

    <script>
    $(document).ready(function () {
        //$('.error').hide();
        $('#contactForm')
            .form({
                on: 'blur',
                fields: {

                    fname: {
                        identifier: 'fname',
                        rules: [
                            {
                                type: 'empty',
                                prompt: '@HtmlHelperForView.ViewHelper(Model, 44, "ValidareFName")'
                            }
                        ]
                    },
                    lname: {
                        identifier: 'lname',
                        rules: [
                            {
                                type: 'empty',
                                prompt: '@HtmlHelperForView.ViewHelper(Model, 44, "ValidareLName")'
                            }
                        ]
                    },
                    email: {
                        identifier: 'email',
                        rules: [
                            {
                                type: 'empty',
                                prompt: '@HtmlHelperForView.ViewHelper(Model, 44, "ValidareEmail")'
                            }
                        ]

                    },
                    message: {
                        identifier: 'message',
                        rules: [
                            {
                                type: 'empty',
                                prompt: '@HtmlHelperForView.ViewHelper(Model, 44, "ValidareMessage")'
                            }
                        ]

                    }


                }
            });

    });


    function createUser() {
        var data = $('#newsletterUser').val();
        if (data == "") {
            document.getElementById("NewsletterMessageId").innerHTML = "@HtmlHelperForView.ViewHelper(Model, 44, "ValidareToateCampuri")";
        }
        $.ajax({
            url: '/admin/NewsletterUsers/Create',
            data: {
                Email: data
            }
        }).success(function (response) {
            document.getElementById("NewsletterMessageId").innerHTML = response.message;
            if (response.success == true)
                $('#newsletterUser').val('');
        })
    }

    function submitContactForm() {
        var fname = $('#fname').val();
        var lname = $('#lname').val();
        var email = $('#email').val();
        var message = $('#message').val();
        var valid = $('#g-recaptcha-response').val();
        if (valid == "") {

            document.getElementById("ContactMessageId").innerHTML = "please check";
        }
        else
        if (fname == "" || lname == "" || email == "" || message == "") {
            document.getElementById("ContactMessageId").innerHTML = "@HtmlHelperForView.ViewHelper(Model, 44, "ValidareToateCampuri")";
        }
        else {
            $.ajax({
                type: "POST",
                url: '/Home/ContactForm',
                data: {
                    fname: fname,
                    lname: lname,
                    email: email,
                    message: message,
                    recaptcha: valid
                }, dataType: 'Json',
            }).success(function (data) {
                document.getElementById("ContactMessageId").innerHTML = data.message;
                grecaptcha.reset();
                if (data.success == true) {
                    $('#fname').val('');
                    $('#lname').val('');
                    $('#email').val('');
                    $('#message').val('');


                }

            })
        }


    }

    </script>
    @section Scripts{
        <script src="~/js/semantic.min.js"></script>
    }
