<section class="section-spacing" id="reviews">
    <div class="container">
        <div class="row">
            <div class="section-title text-center">
                <h2>@HtmlHelperForView.ViewHelper(Model, 40, "HeaderDescription")</h2>
                <p>@HtmlHelperForView.ViewHelper(Model, 40, "HeaderSubtitle")</p>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-md-8 col-md-offset-2">
                <div id="testimonials-carousel" class="carousel slide" data-ride="carousel" data-pause="false">

                    <div class="carousel-inner">
                        @foreach (var item in listReviews.Where(x => x.Name == "Review"))
                        {
                            var _OrderIndex = item.OrderIndex;
                            var active = listReviews.Where(x => x.Name == "Review").Min(x => x.OrderIndex) == item.OrderIndex ? "active" : "";

                            <div class="item text-center bg-img @active" style="">
                                <div class="overlay"></div>
                                <div class="user-img">
                                    <img src="@HtmlHelperForView.ImageViewHelper(listReviews, 4, _OrderIndex, "ReviewImage")" alt="ReviewImage">
                                </div>
                                <div class="tesimonial-text">
                                    <p>@HtmlHelperForView.ImageViewHelper(listReviews, 2, _OrderIndex, "Review")</p>
                                    <h5>@HtmlHelperForView.ImageViewHelper(listReviews, 1, _OrderIndex, "ReviewAuthor")</h5>
                                    <div class="ratting">
                                        @{
                                            int stars = Convert.ToInt32(HtmlHelperForView.ImageViewHelper(listReviews, 1, _OrderIndex, "ReviewStars"));
                                            for (int i = 0; i < stars; i++)
                                            {
                                                <i class="fa fa-star"></i>
                                            }
                                        }
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                    <ol class="video-carousel-indicators">
                        @{
                            var count = listReviews.Where(x => x.Name == "Review").Count();
                            for (int i = 0; i < count; i++)
                            {
                                var active = i == 0 ? "active" : "";
                                <li data-target="#testimonials-carousel" data-slide-to="@i" class="@active"></li>
                            }
                        }
                    </ol>
                </div>

            </div>
        </div>
    </div>
</section>

<!-- end pricing -->

<section id="map" class="no-padding"></section>
